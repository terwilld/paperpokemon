<%- include('../_layouts/adminheader') %>

<h2 class="page-title">Clicks</h2>



<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" />
<link rel="stylesheet" type="text/css" href="/css/users/users.css" />


<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title text-uppercase mb-0">View Clicks</h5>
                </div>
                <div class="table-responsive">
                    <table class="table no-wrap user-table mb-0">
                        <thead>
                            <tr>
                                <th scope="col" class="border-0 text-uppercase font-medium pl-4">Click ID: </th>
                                <th scope="col" class="border-0 text-uppercase font-medium">PublisherID</th>
                                <th scope="col" class="border-0 text-uppercase font-medium">Converted?</th>
                                <th scope="col" class="border-0 text-uppercase font-medium">External Order Number</th>
                                <th scope="col" class="border-0 text-uppercase font-medium">Clicked Time</th>
                                <th scope="col" class="border-0 text-uppercase font-medium">Converted Time</th>
                            </tr>
                        </thead>
                        <tbody>

                            <%  for (let Click = 0; Click< Clicks.length; Click++) { %>
                            <tr>
                                <td class="pl-4"><%=Clicks[Click].clickID%></td>

                                <td>
                                    <h5 class="font-medium mb-0"><%=Clicks[Click].publisherID%></h5>
                                </td>

                                <td>
                                    <span class="text-muted"><%=Clicks[Click].converted%></span><br>
                                </td>
                                <td>

                                    <span class="text-muted"><%=Clicks[Click].externalOrderNumber%></span>
                                </td>
                                <td>
                                    <span class="text-muted"><%=Clicks[Click].timeClicked%></span><br>

                                </td>
                                <td>
                                    <span class="text-muted"><%=Clicks[Click].timeConverted%></span><br>
                                </td>
                            </tr>
                            <% }%>

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

</div>

<script src=" https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
<script src="/js/users/users.js"></script>


<%- include('../_layouts/adminfooter') %>